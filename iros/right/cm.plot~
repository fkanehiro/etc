set term postscript enhanced color
set size 0.5,0.5
set output "rightcm.eps"

set xlabel "Time[s]"
set ylabel "ZMP[m]"
plot "orig/com.dat" u 1:2 w l t "X position(original)", "woca/com.dat" u 1:2 w l t "X position(modified)", "orig/com.dat" u 1:3 w l t "Y position(original)", "woca/com.dat" u 1:3 w l t "Y position(modified)"

