#set size 0.5,0.5
set output "righttime.eps"
set key left top

set xlabel "Time[s]"
set ylabel "Processing Time[ms]"
plot "full/tm.dat" u 1:(($2+$3)*1000) w l lw 5 t "total","full/tm.dat" u 1:($2*1000) w l lw 5 t "finding spheres","full/tm.dat" u 1:($3*1000) w l lw 5 t "solving IK"


